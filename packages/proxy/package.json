    "siwe": "^2.3.2",
    "viem": "^2.21.32",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.6",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^8.12.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
